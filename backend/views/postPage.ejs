<main>
  <div class="section outer-div light-mode" id="postContainer">
    <div class="post-header">
      <% if (post.isSponsored) { %>
        <div class="sponsored-label"><i class="fas fa-bullhorn"></i>&nbsp; Sponsored</div>
      <% } %>

      <div class="post-image">
        <img src="<%- post.image ? `http://localhost:3002${post.image}` : 'default-image-path.jpg' %>" 
             title="<%- post.title %>" 
             alt="<%- post.title %>" 
             loading="lazy" />
      </div>

      <div class="post-breadcrumb">
        <a href="/">Home</a>
        <span><i class="fas fa-angle-right"></i></span>
        <a class="post-meta-a" style="color: #3368c6;" href="/api/posts/all/blog-list?postType=<%- encodeURIComponent(post.types.join(', ')) %>">
          <%- post.types.join(", ") %>
        </a>
        <span><i class="fas fa-angle-right"></i></span>
        <a class="post-meta-a" style="color: #3368c6;" href="/api/posts/all/blog-list?category=<%- encodeURIComponent(post.categories.join(', ')) %>">
          <%- post.categories.join(", ") %>
        </a>
        <span><i class="fas fa-angle-right"></i></span>
        <%- post.subCategories.map(subCategory => 
          `<a style="color: #3368c6;" class="post-label-a" href="/api/posts/all/blog-list?subCategory=${encodeURIComponent(subCategory)}">${subCategory}</a>`).join(' <span><i class="fas fa-angle-right"></i></span> ') %>
      </div>

      <h1><a style="color: #007bff;" href="/api/posts/post/<%- post.slug %>"><%- post.title %></a></h1>

      <div class="post-meta-wrapper">
        <span><i class="fa-regular fa-comment fa-flip fa-lg fa-light"></i> 
          <span class="post-meta-detail"><%- post.comments ? post.comments.length : 0 %></span>
        </span>
        <span><i class="fa-regular fa-eye fa-lg fa-light"></i> 
          <span class="post-meta-detail"><%- post.views || 0 %></span>
        </span>
        <span><i class="fa-regular fa-user fa-lg fa-light"></i> 
          <span class="post-meta-detail"><%- post.author ? post.author.fullName : 'Unknown author' %></span>
        </span>
        <span><i class="fa-regular fa-clock fa-lg fa-light"></i> 
          <span class="post-meta-detail"><%- formatPostDate(new Date(post.createdAt)) %></span>
        </span>
      </div>
    </div>

    <div class="post-body entry-content" itemprop="description articleBody">
      <%- post.content %>
    </div>

    <div class="post-footer">
      <div class="post-label post-section">
        <h4 class="post-section-title">
          <i class="fas fa-tag"></i> Labels
        </h4>
        <%- post.tags.map(tag => 
          `<a class="post-label-a" href="/api/posts/all/blog-list?tag=${encodeURIComponent(tag)}">
            <span class="bg label-name">${tag}</span>
          </a>`
        ).join(" ") %>
      </div>

      <div class="post-section post-share-buttons">
        <h4 class="post-section-title">
          <i class="fas fa-share-alt"></i>
          SHARE (<span id="share-count"><%- post.shares %></span>):
        </h4>
        <div class="post-share-buttons-holder">
          <% let shareUrl = `https://agapelovemedicallaboratory.com/api/posts/post/${post.slug}`; %>
          <a href="https://www.facebook.com/sharer.php?u=<%- shareUrl %>" target="_blank" class="post-label p-facebook">
            <span class="bg label-name"><i class="fab fa-facebook-f"></i> Share</span>
          </a>
          <a href="https://twitter.com/share?url=<%- shareUrl %>" target="_blank" class="post-label twitter">
            <span class="bg label-name"><i class="fab fa-x"></i> Twitter</span>
          </a>
          <a href="https://www.linkedin.com/shareArticle?mini=true&url=<%- shareUrl %>" target="_blank" class="post-label linkedin">
            <span class="bg label-name"><i class="fab fa-linkedin-in"></i> Share</span>
          </a>
        </div>
      </div>
    </div>

    <div class="comments-section">
      <h4><i class="fas fa-comments"></i> Comments</h4>
      <div class="comment-toggle-buttons">
        <button onclick="showSiteComments()">Site (<%- post.comments ? post.comments.length : 0 %>)</button>
        <button onclick="showFacebookComments()">Facebook</button>
      </div>

      <div id="siteCommentsSection">
        <% if (Array.isArray(post.comments)) { %>
          <% post.comments.forEach(comment => { %>
            <div class="comment">
              <div class="avatar-image-container">
                <img src="/images/default-user-icon.png" alt="Avatar">
              </div>
              <div class="comment-block">
                <cite><%- comment.commenter || "Anonymous" %></cite>
                <span class="comment-date"><%- formatCommentDate(new Date(comment.createdAt)) %></span>
                <p><%- comment.content %></p>
                <button onclick="showReplyInput('<%= post._id %>', '<%= comment._id %>')">Reply</button>
              </div>

              <% if (Array.isArray(comment.replies)) { %>
                <div class="comment-replies-container">
                  <% comment.replies.forEach(reply => { %>
                    <div class="comment reply">
                      <div class="avatar-image-container">
                        <img src="/images/default-user-icon.png" alt="Avatar">
                      </div>
                      <div class="comment-block">
                        <cite><%- reply.replyer || "Anonymous" %></cite>
                        <span class="comment-date"><%- formatCommentDate(new Date(reply.createdAt)) %></span>
                        <p><%- reply.content %></p>
                      </div>
                    </div>
                  <% }) %>
                </div>
              <% } %>
            </div>
          <% }) %>
        <% } %>

        <textarea id="newComment" placeholder="Add a comment"></textarea>
        <button onclick="addComment('<%= post._id %>')">Post Comment</button>
      </div>

      <div id="facebookCommentsSection" style="display: none;">
        <div class="fb-comments" data-href="http://localhost:3002/api/posts/<%= post._id %>" data-width="100%" data-numposts="5"></div>
      </div>
    </div>
  </div>
</main>


