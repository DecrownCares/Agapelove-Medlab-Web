                        <!-- <div class="owl-carousel testimonial-carousel wow fadeIn" data-wow-delay="0.1s" id="testimonialContainer">
                           
                        </div> -->

                        <div class="owl-carousel testimonial-carousel" id="testimonialContainer">
                            <% if (reviews.length > 0) { %>
                                <% reviews.forEach(review => { %>
                                    <div class="testimonial-item">
                                        <div class="icon-box-primary mb-4">
                                            <i class="bi bi-chat-left-quote text-dark"></i>
                                        </div>
                                        <p><strong><%= review.reviewType %></strong></p>
                                        <p class="fs-5 mb-4"><%= review.review %></p>
                                        <div class="d-flex align-items-center">
                                            <img class="flex-shrink-0 rounded-circle" 
                                                 src="<%= review.image ? review.image : 'img/default-user-icon.png' %>" 
                                                 alt="User Image" 
                                                 style="width: 50px; height: 50px; object-fit: cover;">
                                            <div class="ps-3">
                                                <h5 class="mb-1"><%= review.name %></h5>
                                            </div>
                                        </div>
                                    </div>
                                <% }); %>
                            <% } else { %>
                                <p class="text-center">No reviews yet.</p>
                            <% } %>
                        </div>
                    </div>
                </div>

                <div class="col-lg-6 wow fadeInUp" data-wow-delay="0.1s">
                    <h2 class="mb-4 display-6 text-white">Share Your Experience</h2>
                    <p class="mb-4">We value your feedback! Please share your experience and let us know how we can improve.</p>
                    <form id="reviewForm" enctype="multipart/form-data">
                        <div class="row g-3">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" name="name" id="name" placeholder="Your Name" required>
                                    <label for="name">Your Name <span style="color: #f44336;">*</span></label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="email" class="form-control" name="email" id="email" placeholder="Your Email" required>
                                    <label for="email">Your Email <span style="color: #f44336;">*</span></label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-floating">
                                    <select class="form-select" id="reviewType" name="review_type">
                                        <option value="Praise">Praise</option>
                                        <option value="Suggestion">Suggestion</option>
                                        <option value="Concern">Concern</option>
                                        <option value="Other">Other</option>
                                    </select>
                                    <label for="appointmentType">Review Type</label>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating">
                                    <textarea class="form-control" placeholder="Write your review here" name="review" id="review" style="height: 150px" required></textarea>
                                    <label for="review">Your Review <span style="color: #f44336;">*</span></label>
                                </div>
                            </div>
                            <div class="col-12">
                                <label for="image" class="form-label">Upload an Image (Optional)</label>
                                <input type="file" class="form-control" name="image" id="image" accept="image/*">
                            </div>
                            <div class="col-12 text-center">
                                <button class="btn btn-primary w-100 py-3" type="submit">Submit Review</button>
                            </div>
                        </div>
                    </form>
                </div>
                
            </div>
        </div>
    </div>

